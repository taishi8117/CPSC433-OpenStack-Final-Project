package project;

public class APIHandler {
	private Controller controller;
	
	/**
	 * Creates a new tenant and network. Should be called when the corresponding API is called
	 * @return tenantID
	 */
	//TODO error handling when there is an error??
	public int createTenantAndNetwork(String networkName) {
		long tenantID = controller.registerNewTenant();
		controller.registerNewNetwork(tenantID, networkName);
		return tenantID;
	}
	

}
